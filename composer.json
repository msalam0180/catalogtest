    "drupal/migrate_source_csv": "^3.4",
    "drupal/migrate_tools": "^5.0",
    "drupal/multiple_fields_remove_button": "^1.0@alpha",
    "drupal/no_autocomplete": "^1.6",
    "drupal/node_title_help_text": "^1.0",
    "drupal/node_title_validation": "^1.22",
    "drupal/nodeaccess": "^1.1",
    "drupal/paragraphs": "^1.11",
    "drupal/pathauto": "^1.6",
    "drupal/plotly_js": "^1.6",
    "drupal/r4032login": "^2.1",
    "drupal/rabbit_hole": "^1.0@beta",
    "drupal/recreate_block_content": "^2.0",
    "drupal/redirect": "^1.5",
    "drupal/role_log": "^1.1",
    "drupal/search_api": "^1.15",
    "drupal/search_api_autocomplete": "^1.2",
    "drupal/search_api_exclude_entity": "^1.0",
    "drupal/seckit": "^2.0",
    "drupal/simple_oauth": "^5.2",
    "drupal/simplesamlphp_auth": "^3.2",
    "drupal/subpathauto": "^1.0",
    "drupal/superfish": "^1.3",
    "drupal/taxonomy_manager": "^2.0",
    "drupal/tvi": "^1.0@beta",
    "drupal/twig_tweak": "^2.0",
    "drupal/view_unpublished": "^1.0",
    "drupal/viewfield": "^3.0@alpha",
    "drupal/views_cm_current_state": "^2.0",
    "drupal/views_data_export": "^1.0",
    "drupal/views_list_sort": "^1.1",
    "drupal/views_tree": "^2.0@alpha",
    "drupal/weight": "^3.1",
    "drupal/workbench_email": "^2.0",
    "drush/drush": "10.5",
    "kint-php/kint": "^3.3",
    "league/csv": "^9.7.1",
    "mnsami/composer-custom-directory-installer": "^2.0",
    "simplesamlphp/simplesamlphp": "^1.18.2",
    "w8tcha/ckeditor-wordcount-plugin": "^1.17",
    "wikimedia/composer-merge-plugin": "^2.0",
    "zodiacmedia/drupal-libraries-installer": "^1.5"
  },
  "require-dev": {
    "behat/mink": "^1.7",
    "phpunit/phpunit": "^9.5"
  },
  "conflict": {
    "drupal/core": "7.*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "drupal-scaffold": {
      "file-mapping": {
          "[web-root]/robots.txt": {
              "mode": "skip"
          },
          "[web-root]/.eslintignore":{
              "mode": "skip"
          },
          "[web-root]/.eslintrc.json":{
            "mode": "skip"
          },
          "[web-root]/.htaccess":{
              "mode": "skip"
          },
          "[web-root]/sites/development.services.yml": {
              "mode": "skip"
          }
      },
      "locations": {
        "web-root": "./docroot"
      }
    },
    "installer-paths": {
      "docroot/core": ["type:drupal-core"],
      "docroot/drush/{$name}": ["type:drupal-drush"],
      "docroot/libraries/{$name}": ["type:drupal-library", "harvesthq/chosen"],
      "docroot/modules/contrib/{$name}": ["type:drupal-module"],
      "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
      "docroot/themes/contrib/{$name}": ["type:drupal-theme"]
    },
    "merge-plugin": {
      "include": [
          "docroot/modules/contrib/ckwordcount/composer.libraries.json"
      ]
    },
    "drupal-libraries": {
      "fontawesome": "https://github.com/FortAwesome/Font-Awesome/archive/refs/tags/5.12.0.zip",
      "jquery-ui-touch-punch": "https://github.com/furf/jquery-ui-touch-punch/archive/refs/heads/master.zip",
      "notification": "https://download.ckeditor.com/notification/releases/notification_4.18.0.zip",
      "superfish": "https://github.com/mehrpadin/Superfish-for-Drupal/archive/refs/tags/2.1.zip"
    },
    "enable-patching": true,
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "patches": {
      "drupal/better_exposed_filters": {
        "Custom patch: DSITE-7066 - Fix reset button not showing.": "docroot/modules/contrib/patches/dsite-7066-reset-button-not-showing.patch"
      },
      "drupal/core": {
        "Custom patch: DSITE-2260 - Instructions is not provided for field marked with an asterisk.": "docroot/modules/contrib/patches/dsite-2260-ckeditor-wysiwyg-required.patch",
        "Custom patch: quickedit entity getFieldDefinition error": "docroot/modules/contrib/patches/quickedit-entity-getFieldDefinition-fix.patch",
        "Drupal Issue: https://www.drupal.org/project/drupal/issues/2230909": "docroot/modules/contrib/patches/drupal-simple-decimal-validation-2230909-234.patch",
        "Drupal Issue: https://www.drupal.org/project/drupal/issues/2941712": "docroot/modules/contrib/patches/2941712-8-ConfigInstaller.patch",
        "Drupal Issue: https://www.drupal.org/project/drupal/issues/2999370 (DSITE-6488)": "docroot/modules/contrib/patches/media_name_overriden_with_source_image_filename.patch",
        "Drupal issue: https://www.drupal.org/project/drupal/issues/3114467": "docroot/modules/contrib/patches/core-3114467-16.patch"
      },
      "drupal/cshs": {
        "Custom patch: DSITE-4036: Add cshs missing labels, DSITE-5757 & DSITE-5756: Add cshs missing attributes - describedby and required.": "docroot/modules/contrib/patches/cshs_aira_and_label.patch"
      },
      "drupal/csv_serialization": {
        "Drupal Issue: https://www.drupal.org/project/csv_serialization/issues/3163819": "docroot/modules/contrib/patches/csv_serialization-3163819_0.patch"
      },
      "drupal/csv_importer": {
        "Drupal Issue: https://www.drupal.org/project/csv_importer/issues/3000137": "docroot/modules/contrib/patches/reroll_diff_9-16.patch"
      },
      "drupal/ctools": {
        "Fix Cannot add contexts whose entity ids are not numeric see https://www.drupal.org/project/ctools/issues/2696819": "docroot/modules/contrib/patches/2696819-26.non-numeric-context-id.patch"
      },
      "drupal/entity_clone": {
        "Fix memory limit issue with cloning referenced entities: https://www.drupal.org/project/entity_clone/issues/3010626": "docroot/modules/contrib/patches/entity_clone-avoid_unnecessary_recursion_3010626-13.patch"
      },
      "drupal/entity_extra_field": {
        "Extra fields not being added to a field group: https://www.drupal.org/project/entity_extra_field/issues/3155613": "docroot/modules/contrib/patches/entity-extra-field-3155613-1.patch"
      },
      "drupal/environment_indicator": {
        "Custom patch: DSITE-3647 - Environment Switcher doesn't stay expeded on search result page (Fixing on-click event)": "docroot/modules/contrib/patches/dsite-3647-fix-environment-indicator-js-on-click.patch"
      },
      "drupal/http_response_headers": {
        "Drupal issue: https://www.drupal.org/project/http_response_headers/issues/3193303": "docroot/modules/contrib/patches/http_response_headers-3193303.patch",
        "Drupal issue: https://www.drupal.org/project/http_response_headers/issues/3193304": "docroot/modules/contrib/patches/http_response_headers-3193304.patch"
      },
      "drupal/inline_entity_form": {
        "Drupal issue: https://www.drupal.org/project/inline_entity_form/issues/2932492": "docroot/modules/contrib/patches/inline_entity_form-required-field-2932492.patch"
      },
      "drupal/masquerade": {
        "Custom: Patch masquerade check for user to have all perms": "docroot/modules/contrib/patches/patch-masquerade-roles-check.patch",
        "Drupal issue: https://www.drupal.org/project/masquerade/issues/2679088": "docroot/modules/contrib/patches/masquerade-2679088-redirect.patch",
        "Drupal issue: https://www.drupal.org/project/masquerade/issues/2900413": "docroot/modules/contrib/patches/2900413-8.show-unmasquerade-link-in-block.patch",
        "Drupal issue: https://www.drupal.org/project/masquerade/issues/3003796": "docroot/modules/contrib/patches/2019-4-5-filterbyrole.patch"
      },
      "drupal/nodeaccess": {
        "Drupal issue: https://www.drupal.org/project/nodeaccess/issues/3116479": "docroot/modules/contrib/patches/nodeaccess-adding-reset-button-3116479-3.patch"
      },
      "drupal/node_title_validation": {
        "Drupal issue: https://www.drupal.org/project/node_title_validation/issues/3243402": "docroot/modules/contrib/patches/node-title-validator-3243402.patch"
      },
      "drupal/plotly_js": {
        "Custom patch: DSITE-2249 - Limiting graph types and fixing adding additional series render.": "docroot/modules/contrib/patches/dsite-2249-plotly-js-fix-for-adding-series.patch",
        "D9 custom patch": "docroot/modules/contrib/patches/dsite-6705-plotly_js-d9.patch"
      },
      "drupal/r4032login": {
        "Custom patch: DSITE-6775 - Adding patch to add / back into URL parameters reverting Drupal.org issue https://www.drupal.org/project/r4032login/issues/3102467": "docroot/modules/contrib/patches/dsite-6775-r4032login-to-add-forward-slash.patch"
      },
      "drupal/simplesamlphp_auth": {
        "Drupal issue: https://www.drupal.org/project/simplesamlphp_auth/issues/2124117 - masquerade and saml compatibility": "docroot/modules/contrib/patches/2124117-6-reroll-14.patch",
        "Drupal issue: https://www.drupal.org/project/simplesamlphp_auth/issues/2983154": "docroot/modules/contrib/patches/simplesamlphp_auth-return-to-destination-parameter-2983154-8-D8.patch",
        "Custom DSITE-7233: Forces simplesaml redirection to requested url": "docroot/modules/contrib/patches/saml_redirect-DSITE7233.patch"
      }
    }
  },
  "repositories": [
    {
      "type": "package",
      "package": [
          {
            "name": "drupal/plotly_js",
            "type": "drupal-module",
            "version": "1.6",
            "source": {
                "type": "git",
                "url": "https://git.drupalcode.org/project/plotly_js.git",
                "reference": "ac3260835e455d623ac7c7b4060fe8ae80064151"
            }
          }
      ]
    },
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "config": {
    "sort-packages": true,
    "discard-changes": true,
    "platform": {
      "php": "8.0"
    }
  }
}
