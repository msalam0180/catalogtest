    "drupal/entity": "^1.0",
    "drupal/entity_browser": "^2.6",
    "drupal/entity_clone": "^1.0",
    "drupal/entity_extra_field": "^2.0@alpha",
    "drupal/entity_reference_labels": "^2.0",
    "drupal/entity_reference_revisions": "^1.7",
    "drupal/environment_indicator": "^4.0",
    "drupal/externalauth": "^1.2",
    "drupal/facets": "^2.0",
    "drupal/features": "^3.7",
    "drupal/feeds": "^3.0@beta",
    "drupal/feeds_tamper": "^2.0@beta",
    "drupal/field_delimiter": "^1.0",
    "drupal/field_display_label": "^1.0",
    "drupal/field_group": "^3.0",
    "drupal/field_permissions": "^1.0",
    "drupal/field_validation": "^1.0",
    "drupal/fixed_text_link_formatter": "1.x-dev@dev",
    "drupal/flag": "^4.0",
    "drupal/flood_settings": "^1.0",
    "drupal/fontawesome": "^2.15",
    "drupal/google_analytics": "^2.4",
    "drupal/google_tag": "^1.4",
    "drupal/hierarchical_term_formatter": "^1.1",
    "drupal/http_response_headers": "^2.0",
    "drupal/inline_entity_form": "^1.0",
    "drupal/jsonapi_earlyrendering_workaround": "^1.0",
    "drupal/layout_builder_modal": "^1.0",
    "drupal/layout_builder_restrictions": "^2.5",
    "drupal/link_attributes": "^1.10",
    "drupal/linkit": "^6.0@beta",
    "drupal/maillog": "1.x-dev",
    "drupal/masquerade": "^2.0",
    "drupal/media_entity_browser": "^2.0",
    "drupal/memcache": "^2.0",
    "drupal/metatag": "^1.9",
